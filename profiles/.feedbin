feedbin-start(){
    redis-start
    es-start
    psql-start
    mem-start
    tmuxinator start feedbin -n feedbin

}

#--------------------------------------

feedbin-stop(){
    tmux kill-session -t feedbin
    redis-stop
    es-stop
    psql-stop
    mem-stop
    pm2 stop server
}


